{
"--file-share":
  {
    "capacity": "1024GB",
    "name": "hpccdata",
    "nfs-export-options": [
      {
        "access-mode": "READ_WRITE",
        "squash-mode": "ROOT_SQUASH",
        "anon_uid": 101,
        "anon_gid": 101
      },
      {
        "access-mode": "READ_WRITE",
        "ip-ranges": [
        "192.168.0.0/24"
        ],
        "squash-mode": "NO_ROOT_SQUASH"
      }
    ]
  }
}
